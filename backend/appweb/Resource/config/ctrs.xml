<?xml version="1.0" encoding="utf-8"?>
<ws xmlns:ws="http://www.west-soft.com/ws" >
  <root>
    <!--控件-->
    <!--日期控件-->
    <row>
      <key>ws.def.dateinput.ws</key>
      <value>
        <![CDATA[
      <label>
        %inj.labeltitle%:
      </label>
      <span>
        <input value='%inj.value%' id='%inj.id%' style="width:82px;height:20px" class="Wdate" onfocus="WdatePicker()">
      </span>
       ]]>
      </value>
    </row>

    <!--label（参数：labeltitle）-->
    <row>
      <key>ws.def.label.ws</key>
      <value>
        <![CDATA[
      <label>
        %inj.labeltitle%:
      </label>
      ]]>
      </value>
    </row>
    <!--空label（参数：labeltitle）-->
    <row>
      <key>ws.def.emtlabel.ws</key>
      <value>
        <![CDATA[
      <label>

      </label>
      ]]>
      </value>
    </row>

    <!--一般的输入框（参数：title、type、id、value、spanclass、class、disabled、tabindex、maxlength）-->
    <row>
      <key>ws.def.defaultinput.ws</key>
      <value>
        <![CDATA[
      <span class="%inj.spanclass%">
        <input title="%inj.title%" type="%inj.type%" id="%inj.id%" value="%inj.value%" class="%inj.class%" maxlength="%inj.maxlength%" />
      </span>
      ]]>
      </value>
    </row>

    <!--输入框（参数：labeltitle、title、type、id、value、class、disabled、tabindex、maxlength）-->
    <row>
      <key>ws.def.input.ws</key>
      <value>
        <![CDATA[
      <label>
        %inj.labeltitle%:
      </label>
      <span>
        <input title="%inj.title%" type="%inj.type%" id="%inj.id%" value="%inj.value%" class="%inj.class%" maxlength="%inj.maxlength%" />
      </span>
      ]]>
      </value>
    </row>

    <!--文本域（参数：id、name、class、cols、rows、tabindex、title）-->
    <row>
      <key>ws.def.textarea.ws</key>
      <value>
        <![CDATA[      
      <textarea id="%inj.id%" name="%inj.name%" class="%inj.class%" cols="%inj.cols%" rows="%inj.rows%" tabindex="%inj.tabindex%" title="%inj.title%"></textarea>
      ]]>
      </value>
    </row>

    <!--数据导入模板下拉框-->
    <row>
      <key>ws.def.modelddlist.ws</key>
      <value>
        <![CDATA[
      <label>
        %inj.labeltitle%:
      </label>
      <span class="spanselect">
        <select id="%inj.id%" name="%inj.name%" dir="ltr" title="%inj.title%" style="width:144px;">         
            ws.def.getxmldata{"defid":"ctrl_sys_dict_importmodel_list","fmtid":"ctrl_selectoption","strJson":{'common': { 'currpage': 1, 'pagesize': 100,'where': [{'col':'cate','logic':'=','val':'%inj.cate%','andor':'and'},{'col':'val','logic':'=','val':'%inj.val%','andor':''}]}}}.ws          
        </select> 
      </span>
      ]]>
      </value>
    </row>

    <!--下拉框（参数：labeltitle、id、name、title、cate、select）-->
    <row>
      <key>ws.def.ddlist.ws</key>
      <value>
        <![CDATA[
      <label>
        %inj.labeltitle%:
      </label>
      <span class="spanselect">
        <select id="%inj.id%" name="%inj.name%" dir="ltr" title="%inj.title%" style="width:120px;">         
            ws.def.getxmldata{"defid":"ctrl_sys_dict_list","fmtid":"ctrl_selectoption","strJson":{'common': { 'currpage': 1, 'pagesize': 100,'where': [{'col':'s.cate','logic':'=','val':'%inj.cate%','andor':'and'},{'col':'s.id','logic':'=','val':'%inj.select%','andor':''}]}}}.ws          
        </select> 
      </span>
      ]]>
      </value>
    </row>

    <!--下拉框（参数：labeltitle、id、name、title、cate、select）-->
    <row>
      <key>ws.def.searchddlist.ws</key>
      <value>
        <![CDATA[
      <label>
        %inj.labeltitle%:
      </label>
      <span class="spanselect">
        <select id="%inj.id%" name="%inj.name%" dir="ltr" title="%inj.title%" style="width:100px;">         
            ws.def.getxmldata{"defid":"ctrl_sys_dict_list","fmtid":"ctrl_searchselectoption","strJson":{'common': { 'currpage': 1, 'pagesize': 100,'where': [{'col':'s.cate','logic':'=','val':'%inj.cate%','andor':'and'},{'col':'s.id','logic':'=','val':'%inj.select%','andor':''}]}}}.ws          
        </select> 
      </span>
      ]]>
      </value>
    </row>

    <!--复选框（参数：labeltitle、id、name、value、checked）-->
    <row>
      <key>ws.def.checkbox.ws</key>
      <value>
        <![CDATA[
      <span class="spancheckbox">
        <input type="checkbox" id="%inj.id%" name="%inj.name%" value="%inj.value%" %inj.checked%/>
      </span>
      <label class="spancheckbox">
        %inj.labeltitle%
      </label>
      ]]>
      </value>
    </row>

    <!--多选复选框（参数：labeltitle、id、cate、list）-->
    <row>
      <key>ws.def.morecheckbox.ws</key>
      <value>
        <![CDATA[
      <label>
        %inj.labeltitle%:
      </label>
      <span id="%inj.id%" class="spanmorecheckbox">
        ws.def.getxmldata{"defid":"ctrl_sys_dict_%inj.cate%_list","fmtid":"ctrl_checkbox","strJson":{'common': { 'currpage': 1, 'pagesize': 100,'where': [{'col':'s.cate','logic':'=','val':'%inj.cate%','andor':'and'},%inj.list%]}}}.ws
      </span>
      ]]>
      </value>
    </row>

    <!--多选下拉框（参数：labeltitle、id、cate、list）-->
    <row>
      <key>ws.def.moreselect.ws</key>
      <value>
        <![CDATA[
      <div class="moreselectcontainer">
        <label>
          %inj.labeltitle%:
        </label>
        <div id="%inj.id%" class="moreselect">
          ws.def.getxmldata{"defid":"ctrl_sys_dict_%inj.cate%_list","fmtid":"ctrl_moreselect","strJson":{'common': { 'currpage': 1, 'pagesize': 100,'where': [{'col':'s.cate','logic':'=','val':'%inj.cate%','andor':'and'},%inj.list%]}}}.ws
        </div>
      </div>
      ]]>
      </value>
    </row>

    <row>
      <key>ws.def.treeselect.ws</key>
      <value>
        <![CDATA[
      <div class="moreselectcontainer">
        <label>
          %inj.labeltitle%:
        </label>
        <div id="%inj.id%" class="moreselect">
          ws.def.getxmldata{"defid":"ctrl_sys_dict_%inj.cate%_list","fmtid":"ctrl_treeselect","strJson":{'common': { 'currpage': 1, 'pagesize': 100,'where': [%inj.list%]}}}.ws
        </div>
      </div>
      ]]>
      </value>
    </row>

    <row>
      <key>ws.def.scale.ws</key>
      <value>
        <![CDATA[
      <div class="moreselectcontainer">
        <label>
          %inj.labeltitle%:
        </label>
        <div id="%inj.id%" class="moreselect">
          ws.def.getxmldata{"defid":"ctrl_sys_dict_%inj.cate%_list","fmtid":"ctrl_scale","strJson":{'common': { 'currpage': 1, 'pagesize': 100}}}.ws
        </div>
      </div>
      ]]>
      </value>
    </row>


    <!--单选框-->
    <row>
      <key>ws.def.radio.ws</key>
      <value>
        <![CDATA[
          ws.def.getxmldata{"defid":"ctrl_sys_dict_list","fmtid":"radio","strJson":{'common': { 'currpage': 1, 'pagesize': 100,'where': [{'col':'s.cate','logic':'=','val':'%inj.cate%','andor':'and'},'%inj.list%']}}}.ws
      ]]>
      </value>
    </row>


    <!--文本（参数：title）-->
    <row>
      <key>ws.def.text.ws</key>
      <value>
        <![CDATA[%inj.title%]]>
      </value>
    </row>

    <!--表格选择（参数：iid）-->
    <row>
      <key>ws.def.choose.ws</key>
      <value>
        <![CDATA[<input type='checkbox' iid='%inj.iid%'/>]]>
      </value>
    </row>

    <!--链接（参数：title、id、href、alt）-->
    <row>
      <key>ws.def.link.ws</key>
      <value>
        <![CDATA[<a id='%inj.id%' href='%inj.href%' alt='%inj.alt%'>%inj.title%</a>]]>
      </value>
    </row>

    <!--单击触发事件（参数：title、id、alt、event）-->
    <row>
      <key>ws.def.button.ws</key>
      <value>
        <![CDATA[<a id="%inj.id%" href="#" alt="%inj.alt%" onclick="%inj.event%">%inj.title%</a>]]>
      </value>
    </row>

    <!--图片（参数：id、class、src、alt）-->
    <row>
      <key>ws.def.img.ws</key>
      <value>
        <![CDATA[<img id="%inj.id%" class="%inj.class%" src="%inj.src%" alt="%inj.alt%"/>]]>
      </value>
    </row>

    <!--选择（）-->
    <row>
      <key>ws.def.select.ws</key>
      <value>
        <![CDATA[ws.def.getxmldata{"defid":"ctrl_sys_dict_list","fmtid":"ctrl_table_select","strJson":{'common': { 'currpage': 1, 'pagesize': 100,'where': [{'col':'s.cate','logic':'=','val':'%inj.cate%','andor':'and'},{'col':'s.id','logic':'=','val':'%inj.select%','andor':''}]}}}.ws]]>
      </value>
    </row>
  </root>
</ws>