<?xml version="1.0" encoding="utf-8"?>


<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="false" >
        <shape>
		    <solid android:color="#FA5000" />
		    <corners
		        android:radius="15dp"/>
		    <stroke
		        android:width="1dp"
		        android:color="#808080" />
		</shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
		    <solid android:color="#FAd0d0" />
		    <corners
		        android:radius="15dp"/>
		    <stroke
		        android:width="1dp"
		        android:color="#808080" />
		</shape>
    </item>
</selector>


